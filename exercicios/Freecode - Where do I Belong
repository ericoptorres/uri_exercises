function getIndexToIns(arr, num) {

    arr.sort((a , b) => a - b)

    let i = 0
    while (i < arr.length){
        if (num <= arr[i]){
            return i
        }
        i++ 
    }
    
    return arr.length
  }

  console.log(getIndexToIns([10, 20, 30, 40, 50], 30))
  
  getIndexToIns([10, 20, 30, 40, 50], 30);