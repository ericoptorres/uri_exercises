const [X , Y] = (lines[0].split(' '))


switch (X) {
  case '1':
    console.log(`Total: R$ ${(Y * 4).toFixed(2)}`)
    break;
  
  case '2':
      console.log(`Total: R$ ${(Y * 4.5).toFixed(2)}`)
      break;

  case '3':
      console.log(`Total: R$ ${(Y * 5).toFixed(2)}`)
      break;

  case '4':
      console.log(`Total: R$ ${(Y * 2).toFixed(2)}`)
      break; 

  case '5':
      console.log(`Total: R$ ${(Y * 1.5).toFixed(2)}`)
      break;

}