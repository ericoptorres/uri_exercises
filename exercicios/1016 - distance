//var input = require('fs').readFileSync('/dev/stdin', 'utf8');
var input = require('fs').readFileSync('stdin', 'utf8');
var lines = input.split('\n');



var distancia = parseInt(lines)

var min = (distancia / 30) * 60

console.log(min, "minutos")



